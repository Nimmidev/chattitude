<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="conversationMemberCell" prefHeight="45.0" prefWidth="287.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="usernameLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Tom Frank" HBox.hgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label fx:id="adminLabel" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" style="-fx-border-radius: 5px;" text="Admin" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
         </HBox.margin>
      </Label>
      <JFXButton fx:id="proDemoteButton" onAction="#proDemoteClick" prefHeight="35.0" prefWidth="71.0" text="ProDemote">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
      </JFXButton>
      <JFXButton fx:id="removeButton" onAction="#removeClick" prefHeight="35.0" prefWidth="35.0" style="-fx-background-image: url(&quot;icons/remove.png&quot;); -fx-background-size: contain;" textAlignment="CENTER" HBox.hgrow="NEVER" />
   </children>
   <padding>
      <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
   </padding>
</HBox>
